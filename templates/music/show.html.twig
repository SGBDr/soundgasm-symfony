{% extends 'base.html.twig' %}

{% block title %}Music{% endblock %}

{% block body %}
    <h1>Music</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ music.id }}</td>
            </tr>
            <tr>
                <th>Music_id</th>
                <td>{{ music.musicId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ music.name }}</td>
            </tr>
            <tr>
                <th>Rep_image</th>
                <td>{{ music.repImage }}</td>
            </tr>
            <tr>
                <th>Track</th>
                <td>{{ music.track }}</td>
            </tr>
            <tr>
                <th>Style</th>
                <td>{{ music.style }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ music.country }}</td>
            </tr>
            <tr>
                <th>Release_date</th>
                <td>{{ music.releaseDate ? music.releaseDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_music_index') }}">back to list</a>

    <a href="{{ path('app_music_edit', {'id': music.id}) }}">edit</a>

    {{ include('music/_delete_form.html.twig') }}
{% endblock %}
